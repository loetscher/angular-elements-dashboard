{
  "name": "dashboard",
  "version": "0.0.0",
  "watch": {
    "copy:ce": {
      "patterns": [
        "dist/external-dashboard-tile/"
      ],
      "extensions": "ts,html,scss, css",
      "quiet": false
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project dashboard -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "npm-watch",
    "start:ce": "ng serve --project external-dashboard-tile -o",
    "build:ce": "npm run bundle:ce",
    "build:ce:watch": "npm run bundle:ce -- --watch",
    "copy:ce": "npm run copy:ce:lib && npm run copy:ce:style && npm run copy:ce:assets",
    "copy:ce:resources": "cpx dist/external-dashboard-tile/**/*.{html,js,css,ttf,map} src/assets/external-dashboard-tile",
    "copy:ce:lib": "cpr dist/external-dashboard-tile/main-es5.js src/assets/external-dashboard-tile/external-dashboard-tile.bundle.js -o",
    "copy:ce:style": "cpr dist/external-dashboard-tile/styles.css src/assets/external-dashboard-tile/styles.css -f -o",
    "copy:ce:assets": "cpr dist/external-dashboard-tile/IcoMoon-Free.ttf src/assets/external-dashboard-tile/IcoMoon-Free.ttf -f -o",
    "copy:ce:local": "cpr dist/external-dashboard-tile/main-es5.js ../../../../web/lean-client/lean-client-biskin/client/src/webComponents/b9e-web-component/main.js -o",
    "bundle:ce": "ng build --project external-dashboard-tile --prod --sourceMap=true --output-hashing none --single-bundle=true --bundle-styles=false",
    "release:ce": "npm run build:ce && npm run copy:ce:resources",
    "release:local:ce": "npm run release:ce && npm run copy:ce:local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/forms": "^8.1.2",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "@swimlane/ngx-charts": "^12.0.1",
    "@webcomponents/custom-elements": "^1.2.4",
    "ag-grid-angular": "^21.1.0",
    "ag-grid-community": "^21.1.0",
    "ag-grid-enterprise": "^21.1.0",
    "application": "^0.1.4",
    "core-js": "^2.5.4",
    "install": "^0.12.1",
    "npm": "^6.5.0",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.801.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/core": "^8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/elements": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "b9e-api": "^2.43.0-SNAPSHOT",
    "codelyzer": "^5.0.1",
    "cpr": "^3.0.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-build-plus": "~8.1.1",
    "node-sass": "^4.12.0",
    "npm-watch": "^0.6.0",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5",
    "webpack": "^4.36.1",
    "webpack-dev-server": "^3.1.14"
  }
}
