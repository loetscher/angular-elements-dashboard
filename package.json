{
  "name": "dashboard",
  "version": "0.0.0",
  "watch": {
    "copy:ce": {
      "patterns": [
        "dist/external-dashboard-tile/"
      ],
      "extensions": "ts,html,scss, css",
      "quiet": false
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project dashboard -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "npm-watch",
    "start:ce": "ng serve --project external-dashboard-tile -o",
    "build:ce": "node ./util/ag-grid-fontface-extractor && npm run bundle:ce",
    "build:ce:watch": "npm run bundle:ce -- --watch",
    "copy:ce": "npm run copy:ce:lib && npm run copy:ce:style && npm run copy:ce:assets",
    "copy:ce:resources": "cpx dist/external-dashboard-tile/**/*.{html,js,css,ttf,map} src/assets/external-dashboard-tile",
    "copy:ce:lib": "cpr dist/external-dashboard-tile/main.js src/assets/external-dashboard-tile/external-dashboard-tile.bundle.js -o",
    "copy:ce:style": "cpr dist/external-dashboard-tile/styles.css src/assets/external-dashboard-tile/styles.css -f -o",
    "copy:ce:assets": "cpr dist/external-dashboard-tile/IcoMoon-Free.ttf src/assets/external-dashboard-tile/IcoMoon-Free.ttf -f -o",
    "copy:ce:local": "cpr dist/external-dashboard-tile/main.js ../../../../web/lean-client/lean-client-biskin/client/src/webComponents/b9e-web-component/main.js -o",
    "bundle:ce": "ng build --project external-dashboard-tile --prod --sourceMap=true --output-hashing none --single-bundle=true --bundle-styles=false",
    "release:ce": "npm run build:ce && npm run copy:ce:resources",
    "release:local:ce": "npm run release:ce && npm run copy:ce:local"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/cdk": "^9.1.1",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/localize": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@bison/biskin-kit": "~1.3.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.0",
    "@swimlane/ngx-charts": "^13.0.2",
    "@webcomponents/custom-elements": "^1.4.0",
    "ag-grid-angular": "^22.0.0",
    "ag-grid-community": "^22.0.0",
    "ag-grid-enterprise": "^22.0.0",
    "application": "^0.1.4",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "date-fns": "^2.10.0",
    "install": "^0.12.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "ngx-toastr": "^12.0.0",
    "npm": "^6.5.0",
    "popper.js": "^1.15.0",
    "rxjs": "^6.5.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.5",
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular-devkit/build-ng-packagr": "~0.900.5",
    "@angular-devkit/core": "^9.0.5",
    "@angular/cli": "~9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/elements": "^9.0.5",
    "@angular/language-service": "^9.0.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "~8.9.4",
    "b9e-api": "2.65.0",
    "codelyzer": "^5.0.1",
    "cpr": "^3.0.1",
    "cpx": "^1.5.0",
    "css": "^2.2.4",
    "css-vars": "^2.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.2",
    "ngx-build-plus": "~9.0.6",
    "node-sass": "^4.12.0",
    "npm-watch": "^0.6.0",
    "protractor": "^5.4.2",
    "tslib": "^1.11.1",
    "tslint": "~5.9.1",
    "typescript": "~3.7.5",
    "webpack": "^4.42.0",
    "webpack-dev-server": "^3.10.3"
  }
}
